{
  "name": "sharr-server",
  "version": "1.0.1",
  "description": "sharing file with node express server",
  "main": "app.js",
  "scripts": {
    "run": "node app.js",
    "client": "cd ../sharrclient && npm start",
    "client-install":"cd ../sharrclient && npm install",
    "client-build":"cd ../sharrclient && npm run build",
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false npm run client-install && npm run client-build"
  },
